[calendars]

[[private]]
path = /Users/joshrasmussen/.local/share/khal/calendars/private
type = calendar

[[newport]]
path = /Users/joshrasmussen/.local/share/khal/calendars/newport
type = calendar

[locale]
timeformat = %I:%M %p
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p

[default]
highlight_event_days = True
show_all_days = True
timedelta = 7d

[highlight_days]
color = brown
default_color = dark green
